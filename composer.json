{
    "name": "johnfischelli/epiclog",
    "description": "Separates Laravel log files into a single file for each log level.",
    "version":"0.1",
    "keywords": ["logs", "log management", "multiple log files"],
    "license": "MIT",
    "authors": [
        {
            "name": "John Fischelli",
            "email": "john.fischelli@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": { 
        "laravel/framework": "5.*",
        "monolog/monolog": "~1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "laracasts/testdummy": "~2.0",
        "laravel/laravel": "5.1"
    },
    "autoload": {
        "psr-4": {
            "EpicLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EpicLog\\Tests\\": "tests"
        }
    }
}
